programa {
  funcao inicio() {
    real qtdfraldaspct
    real custopctfraldas
    real valorunifraldas 
   escreva("Quantas fraldas vem em um pacote? ")
   leia(qtdfraldaspct)
   escreva("Qual o valor do pacote de fraldas? ")
   leia(custopctfraldas)
   valorunifraldas = custopctfraldas / qtdfraldaspct
   escreva("O valor unitário das fraldas é de: ", valorunifraldas) 

  }
}
