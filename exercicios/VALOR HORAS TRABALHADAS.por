programa {
  funcao inicio() {
    inteiro salario, desconto
    real salariofinal
    escreva("valor da hora trabalhada: ")
    leia(salario)
    escreva("quantidade de horas trabalhadas: ")
    leia(desconto)
    salariofinal = salario * desconto
    escreva("salario final é de: ", salariofinal , " zé buceta")

  }
}
